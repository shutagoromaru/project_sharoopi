<div>
    
    <% if @user_profile!=nil%>
        <% if @me.id==@user_profile.user_id%>
            <%= link_to "編集する", edit_user_profile_path, class: "nav-link" %>
        <% end %>
        <div>
            <%= image_tag(@user_profile.image) if @user_profile.image.attached? %>
     
            <p><%= @user_profile.name%></p>
            <p><%= @user_profile.url%></p>
            <p><%= @user_profile.special%></p>
        </div>
            



    <% else %>
        <% if @me.id==params[:user_id].to_i%>
            <%= link_to "登録する", edit_user_profile_path, class: "nav-link" %>
        <% end %>
        <div>        
            <p>未登録</p>
        </div>
    <% end %>
</div>